{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "noEmitOnError": false,
    "skipLibCheck": true,
    "strict": false,
    "allowJs": true,
    "checkJs": false,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "alwaysStrict": false,
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
